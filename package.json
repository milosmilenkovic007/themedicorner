{
  "name": "medicorner-child-theme",
  "version": "1.0.0",
  "description": "Hello Elementor Child Theme for Medicorner with ACF Flexible Content",
  "license": "GPL-3.0-or-later",
  "author": "Medicorner Team",
  "scripts": {
    "start": "npm run watch",
    "watch": "webpack --watch --mode development",
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development --host 0.0.0.0",
    "lint:scss": "stylelint 'assets/scss/**/*.scss'",
    "lint:js": "eslint 'assets/js/**/*.js' --fix",
    "format": "prettier --write 'assets/**/*.{scss,js}'",
    "analyze": "node scripts/analyze-theme.js"
  },
  "dependencies": {
    "jquery": "^3.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@wordpress/env": "^9.0.0",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.57.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.35",
    "postcss-loader": "^7.3.4",
    "prettier": "^3.2.5",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.3",
    "stylelint": "^16.2.1",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
